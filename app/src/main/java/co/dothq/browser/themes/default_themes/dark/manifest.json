{
    "manifest_version": 2,
    "applications": {
        "gecko": {
            "id": "dark@themes.dothq.co"
        }
    },
    "name": "Dark",
    "description": "A theme with a dark color scheme.",
    "author": "Dot HQ",
    "version": "1.1",
    "icons": {
        "32": "icon.svg"
    },
    "theme": {
        "colors": {
            "tab_background_text": "rgb(255, 255, 255)",
            "icons": "rgb(255, 255, 255, 0.95)",
            "frame": "rgb(38, 38, 38)",
            "popup": "rgb(36, 36, 36)",
            "popup_text": "rgb(249, 249, 250)",
            "popup_border": "rgb(48, 48, 53)",
            "tab_line": "rgb(0, 0, 0, 0)",
            "toolbar": "rgb(26, 26, 26)",
            "toolbar_bottom_separator": "rgb(41, 41, 41)",
            "toolbar_field": "#2B2B2B",
            "toolbar_field_text": "#F3F3F3",
            "toolbar_field_border": "#1A1A1A",
            "toolbar_field_focus": "#2B2B2B",
            "toolbar_field_separator": "rgba(255, 255, 255, 0.05)",
            "button_background_hover": "#ffffff14",
            "button_background_active": "#ffffff2e",
            "popup_highlight": "#256EF5",
            "ntp_background": "#1C1C1C",
            "ntp_text": "rgb(249, 249, 250)",
            "sidebar": "#1A1A1A",
            "sidebar_text": "rgb(249, 249, 250)",
            "sidebar_highlight": "white",
            "sidebar_border": "rgba(255, 255, 255, 0.1)"
        }
    },
    "theme_experiment": {
        "stylesheet": "experiment.css"
    }
}
